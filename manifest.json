{
  "name" : "IdeaScape",
  "version" : "1.1",
  "description" : "Think Together",
  "permissions": [
    "browsingData", "background", "http://*/*", "https://*/*", "tabs", "activeTab", "windows", "history", "webNavigation"
  ],
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "background" : {
      "scripts": ["background.js", "jquery-1.11.3.min.js"]
  },
  "manifest_version": 2
}
